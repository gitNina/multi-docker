{
 "AWSEBDockerrunVersion":2,
 "containerDefinitions": [
  {
   "name": "client",
   "image": "ejjj/multi-client",
   "hostname": "client", 
   "essential": false
  },
  {
   "name": "server",
   "image": "ejjj/multi-server",
   "hostname": "api", 
   "essential": false
  },
  {
   "name": "worker",
   "image": "ejjj/multi-worker",
   "hostname": "worker",
   "essential": false
  },
  {
   "name": "nginx",
   "image": "ejjj/multi-nginx", 
   "hostname": "nginx", 
   "essential": true, 
   "portsMappings": [
     {
       "hostPort": 80, 
       "containerPort": 80
     }
    ],
   "links": ["client", "server"]
  }
 ]
}
